<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="even_type_list">
      <field name="name">EventType list</field>
      <field name="model">payment_my.even_type</field>
      <field name="arch" type="xml">
        <tree>
          <field name="Id"/>
          <field name="Code"/>
          <field name="Description"/>
          
        </tree>
      </field>
    </record>
    
	<record id="even_type_form" model="ir.ui.view">
		<field name="name">EventType form</field>
		<field name="model">payment_my.even_type</field>
		<field name="arch" type="xml">
		<form>
			<sheet>
				<group col="2" string="Details">
					<group>
						<field name="Id"/>
						<field name="Code"/>
						<field name="Description"/>
						
					</group>
				</group>
			</sheet>
		</form>
		</field>
	</record>
    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="payment_my.even_type_window">
      <field name="name">EventType window</field>
      <field name="res_model">payment_my.even_type</field>
	  <field name="view_ids" eval="[(5, 0, 0), 
        (0, 0, {'view_mode': 'tree', 'view_id': ref('even_type_list')}),
		(0, 0, {'view_mode': 'form', 'view_id': ref('even_type_form')})
		]"/>
      <field name="view_mode">tree,form</field>
    </record>
    

    <!-- server action to the one above -->
    <!--
    <record model="ir.actions.server" id="payment_my.action_server">
      <field name="name">Payment_my server</field>
      <field name="model_id" ref="model_payment_my_payment_my"/>
